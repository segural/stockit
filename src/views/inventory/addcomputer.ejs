  <body class="hold-transition sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
      <!-- Include Header -->
      <%- include('../partials/header.ejs') %>
      <!--header end-->

      <!--Include Sidebar-->
      <%- include('../partials/sidebar.ejs') %>
      <!--sidebar end-->

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <div class="header-icon">
            <i class="fa fa-user"></i>
          </div>
          <div class="header-title">
            <h1>Inventario</h1>
            <small>Alta de equipo</small>
          </div>
        </section>
        <!-- Main content -->
        <div class="content">
          <div class="row">
             <!-- Form controls -->
             <div class="col-sm-12">
                <div class="card lobicard all_btn_card" id="lobicard-custom-control1" data-sortable="true">
                  <div class="card-header">
                    <div class="card-title custom_title">
                      <h4>Complete los datos del equipo a dar de alta en el inventario</h4>
                    </div>
                  </div>
                    <div class="card-body">
                      <form class="col-sm-6" action="/inventory/addcomputer/add" method="POST" id="form">
                         <div class="form-group">
                            <label>Nombre de equipo</label>
                            <input type="text" class="form-control" name="name" required>
                         </div>
                         <div class="form-group">
                            <label>Sector / Usuario</label>
                            <input type="text" class="form-control" name="sector" required>
                         </div>
                         <div class="form-group">
                            <label>Centro de Atenci√≥n</label>
                            <select class="form-control" name="branch">
                              <% for (branch of branchs) { %>
                              <option value="<%= branch.name %>"> <%= branch.name %> </option>
                              <% } %>
                            </select>
                         </div>
                         <div class="form-group">
                            <label>Propiedades</label>
                            <input type="text" class="form-control" name="properties" required>
                         </div>
                         <div class="form-group">
                            <label>Num. Serie</label>
                            <input type="text" class="form-control" name="serialnumber" required>
                         </div>
                         <div class="form-group">
                           <label>Tipo de equipo</label><br>
                           <label class="radio-inline"> <input name="type" value="desktop" type="radio" checked="checked"> Desktop</label> 
                           <label class="radio-inline"> <input name="type" value="notebook" type="radio"> Notebook</label>
                          </div>
                          <br>
                          <div class="form-group">
                           <label>Dar de baja equipo del stock</label><br>
                           <label class="radio-inline"> <input name="inventory" id="inventory" value="no" type="radio" checked="checked"> NO</label> 
                           <label class="radio-inline"> <input name="inventory" id="inventory" value="si" type="radio"> SI</label>
                          </div>
                          <br>
                         <div class="reset-button">
                            <button onclick="history.back()" class="btn btn-warning"> Volver</button>
                            <button type="submit" class="btn btn-success"> Enviar</button>
                         </div>
                      </form>
                   </div>
                </div>
            </div>
          </div>
       </div>
        <!-- /.content -->
      </div>
      <!-- /.content-wrapper -->
      <!-- Include footer -->
      <%- include('../partials/footer.ejs') %>
      <!-- footer end -->
    </div>
    <!-- ./wrapper -->
    <!-- Start Core Plugins
         ==========================================/===========================-->
    <!-- jQuery -->
    <script src="/assets/plugins/jQuery/jquery-1.12.4.min.js"></script>
    <!-- Bootstrap proper -->
    <script src="/assets/bootstrap/js/popper.min.js"></script>
    <!-- lobicard ui min js -->
    <script src="/assets/plugins/lobipanel/js/jquery-ui.min.js"></script>
    <!-- lobicard ui touch-punch-improved js -->
    <script src="/assets/plugins/lobipanel/js/jquery.ui.touch-punch-improved.js"></script>
    <!-- lobicard tether min js -->
    <script src="/assets/plugins/lobipanel/js/tether.min.js"></script>
    <!-- Bootstrap -->
    <script src="/assets/bootstrap/js/bootstrap.min.js"></script>
    <!-- lobicard js -->
    <script src="/assets/plugins/lobipanel/js/lobicard.js"></script>
    <!-- lobicard highlight js -->
    <script src="/assets/plugins/lobipanel/js/highlight.js"></script>
    <!-- Pace js -->
    <script src="/assets/plugins/pace/pace.min.js"></script>
    <!-- table-export js -->
    <script src="/assets/plugins/table-export/tableExport.js"></script>
    <script src="/assets/plugins/table-export/jquery.base64.js"></script>
    <script src="/assets/plugins/table-export/html2canvas.js"></script>
    <script src="/assets/plugins/table-export/sprintf.js"></script>
    <script src="/assets/plugins/table-export/jspdf.js"></script>
    <script src="/assets/plugins/table-export/base64.js"></script>
    <!-- dataTables js -->
    <script src="/assets/plugins/datatables/dataTables.min.js"></script>
    <script src="/assets/plugins/datatables/DataTables-1.12.1/js/jquery.dataTables.min.js"></script>
    <!-- NiceScroll -->
    <script src="/assets/plugins/slimScroll/jquery.nicescroll.min.js"></script>
    <!-- FastClick -->
    <script src="/assets/plugins/fastclick/fastclick.min.js"></script>
    <!-- CRMadmin frame -->
    <script src="/assets/dist/js/custom.js"></script>
    <!-- End Core Plugins
         =====================================================================-->
    <!-- Start Theme label Script
         =====================================================================-->
    <!-- Dashboard js -->
    <script src="/assets/dist/js/dashboard.js"></script>
    <!-- End Theme label Script
         =====================================================================-->
    <!--this page plugins-->
    <script type="text/javascript" src="/assets/jquery-multi-select/js/jquery.multi-select.js"></script>

    <!--multiselect start + spinner + wysihtml5 scripts-->
    <script src="/js/advanced-form-components.js"></script>

    <script>
      $(document).ready(function () {
        $("#userList").DataTable({
          language: {
            url: "/assets/plugins/datatables/language/es-AR.json",
          },
        });
      });
